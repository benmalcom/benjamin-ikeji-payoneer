{
  "extends": [
    "react-app",
    "react-app/jest",
    "prettier"
  ],
  "globals": {
    "React": true,
    "ReactDOM": true,
    "PropTypes": true,
    "Appcues": true
  },
  "overrides": [
    {
      "files": [
        "**/*.js"
      ],
      "rules": {
        "react-hooks/exhaustive-deps": "off",
        "import/no-anonymous-default-export": "off"
      }
    }
  ]
}
